
// linear search 😅😅😅🤦‍♀️🤦‍♀️
#include<stdio.h>
#define size 100
int main(){
    
    int list[size],n,i,item;
    printf("enter size of list ");
    scanf("%d",&n);

    printf("enter elements of list ");
    for(int i = 0;i < n ; i++){
        scanf("%d",&list[i]);
    }
    printf("enter item to be seached ");
    scanf("%d",&item);
    
    for (i = 0; i < n; i++)
    {
        if (item == list[i])
        {
            printf("Item found at index: %d\n", i);
            break;
        }
    }

       
        
    if (i > n)
    {
        printf("Value not found\n");
    }
     return 0;
}

//uptput:
 enter size of list 5
 enter elementds of list  1 2 3 4 5
 enter item to be seached 3
 inem found at index 2
 
